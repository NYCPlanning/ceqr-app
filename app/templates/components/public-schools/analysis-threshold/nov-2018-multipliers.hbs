<table class="ui table">
  <thead>
    <tr>
      <th colspan="2"></th>
      <th colspan="3">
        Estimated Public School Students Generated by New Residential Units {{ui-popup 
          tagName="i"
          class="no-size book icon"
          content="Table 6-1a: Estimated Public School Students Generated by New Residential Units"
          style="font-size: 1em;"
        }}
      </th>
      <th colspan="2">
        Minimum Number of Residential Units<br>that Trigger Detailed Analyses {{ui-popup 
          tagName="i"
          class="no-size book icon"
          content="Table 6-1: Community Facility Thresholds for Detailed Analyses"
          style="font-size: 1em;"
        }}
      </th>
    </tr>
    
    <tr>
      <th></th>
      <th>School<br>District</th>
      <th>Primary School<br>level per unit<br>(Age 4-10)</th>
      <th>Intermediate School<br>level per unit<br>(Age 11-13)</th>
      <th>High School<br>level per unit<br>(Age 14-17)</th>
      <th>Primary/<br>Intermediate</th>
      <th>High School</th>
    </tr>
  </thead>
  <tbody>

    {{#each multipliers.mn.districts as |m idx|}}
      <tr class="{{m.csd}} {{if (eq m.csd project.district) "active"}}">
        {{#if (eq idx 0)}}
          <td rowspan={{multipliers.mn.count}}>Manhattan</td>
        {{/if}}
        <td>{{m.csd}}</td>
        <td>{{m.ps}}</td>
        <td>{{m.is}}</td>
        <td>{{m.hs}}</td>
        <td>{{m.psisThreshold}}</td>
        <td>{{m.hsThreshold}}</td>
      </tr>
    {{/each}}

    {{#each multipliers.bx.districts as |m idx|}}
      <tr class="{{m.csd}} {{if (eq m.csd project.district) "active"}}">
        {{#if (eq idx 0)}}
          <td rowspan={{multipliers.bx.count}}>Bronx</td>
        {{/if}}
        <td>{{m.csd}}</td>
        <td>{{m.ps}}</td>
        <td>{{m.is}}</td>
        <td>{{m.hs}}</td>
        <td>{{m.psisThreshold}}</td>
        <td>{{m.hsThreshold}}</td>
      </tr>
    {{/each}}

    {{#each multipliers.bk.districts as |m idx|}}
      <tr class="{{m.csd}} {{if (eq m.csd project.district) "active"}}">
        {{#if (eq idx 0)}}
          <td rowspan={{multipliers.bk.count}}>Brooklyn</td>
        {{/if}}
        <td>{{m.csd}}</td>
        <td>{{m.ps}}</td>
        <td>{{m.is}}</td>
        <td>{{m.hs}}</td>
        <td>{{m.psisThreshold}}</td>
        <td>{{m.hsThreshold}}</td>
      </tr>
    {{/each}}

    {{#each multipliers.qn.districts as |m idx|}}
      <tr class="{{m.csd}} {{if (eq m.csd project.district) "active"}}">
        {{#if (eq idx 0)}}
          <td rowspan={{multipliers.qn.count}}>Queens</td>
        {{/if}}
        <td>{{m.csd}}</td>
        <td>{{m.ps}}</td>
        <td>{{m.is}}</td>
        <td>{{m.hs}}</td>
        <td>{{m.psisThreshold}}</td>
        <td>{{m.hsThreshold}}</td>
      </tr>
    {{/each}}

    {{#each multipliers.si.districts as |m idx|}}
      <tr class="{{m.csd}} {{if (eq m.csd project.district) "active"}}">
        {{#if (eq idx 0)}}
          <td rowspan={{multipliers.si.count}}>Staten Island</td>
        {{/if}}
        <td>{{m.csd}}</td>
        <td>{{m.ps}}</td>
        <td>{{m.is}}</td>
        <td>{{m.hs}}</td>
        <td>{{m.psisThreshold}}</td>
        <td>{{m.hsThreshold}}</td>
      </tr>
    {{/each}}

  </tbody>
</table>