<div class="ui grid">
  <div class="ten wide column">
    <ProjectDetails
      @project={{changeset this.model.project this.ProjectValidations}}
      @mapplutoVersions={{this.model.mapplutoVersions}}
      @save={{route-action "save"}}
      @rollback={{route-action "rollback"}}
     />

  </div>
  <div class="six wide column">
    <ProjectUsers @project={{this.model.project}} />
  </div>
</div>
