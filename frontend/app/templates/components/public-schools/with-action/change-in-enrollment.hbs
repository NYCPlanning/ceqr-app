<h4 class="ui header attached top">
  Estimated School Enrollment, Capacity, and Utilization
  {{#if this.analysis.projectionOverMax}}
    <div
      data-tooltip="Build year beyond maximum projections available. Using
        {{this.analysis.maxProjection}}
         enrollment projection."
      data-inverted
      class="ui orange label"
    >
      <i class="warning icon"></i>
      {{this.analysis.maxProjection}}
    </div>
  {{/if}}
</h4>
{{! template-lint-disable table-groups }}
<table class="ui attached bottom celled table">
  <thead>
    <tr>
      <th class="two wide">
        School Level
      </th>
      <th class="two wide">
        Projected Enrollment Without Project
      </th>
      <th class="two wide">
        Students Generated by Project
      </th>
      <th class="one wide">
        Projected Enrollment with Project
      </th>
      <th class="one wide">
        Projected Capacity
      </th>
      <th class="one wide">
        Available Seats
      </th>
      <th class="one wide">
        Utilization (%) with Project
      </th>
      <th class="two wide">
        Change in Utilization (%) from No Action
      </th>
    </tr>
  </thead>
  {{#if this.analysis.psLevelTotals}}
    <tbody>
      <tr>
        <td>
          <strong>
            Primary Schools
          </strong>
        </td>
        <td>
          {{this.analysis.psLevelTotals.enrollNoActionTotal}}
        </td>
        <TdHover @payload={{hash name="studentsFromDev" level="ps"}}>
          {{this.analysis.estEsStudents}}
        </TdHover>
        <td>
          {{this.analysis.psLevelTotals.enrollWithActionTotal}}
        </td>
        <td>
          {{this.analysis.psLevelTotals.capacityWithActionTotal}}
        </td>
        <td>
          {{this.analysis.psLevelTotals.seatsWithActionTotal}}
        </td>
        <TdHover @payload={{hash name="utilizationWithAction" level="ps"}}>
          {{percentage this.analysis.psLevelTotals.utilizationWithActionTotal}}
        </TdHover>
        <TdHover @payload={{hash name="utilizationChange" level="ps"}}>
          {{percentage this.analysis.psLevelTotals.utilizationChangeTotal decimals=2}}
        </TdHover>
      </tr>
    </tbody>
  {{/if}}
  {{#if this.analysis.isLevelTotals}}
    <tbody>
      <tr>
        <td>
          <strong>
            Intermediate Schools
          </strong>
        </td>
        <td>
          {{this.analysis.isLevelTotals.enrollNoActionTotal}}
        </td>
        <TdHover @payload={{hash name="studentsFromDev" level="is"}}>
          {{this.analysis.estIsStudents}}
        </TdHover>
        <td>
          {{this.analysis.isLevelTotals.enrollWithActionTotal}}
        </td>
        <td>
          {{this.analysis.isLevelTotals.capacityWithActionTotal}}
        </td>
        <td>
          {{this.analysis.isLevelTotals.seatsWithActionTotal}}
        </td>
        <TdHover @payload={{hash name="utilizationWithAction" level="is"}}>
          {{percentage this.analysis.isLevelTotals.utilizationWithActionTotal}}
        </TdHover>
        <TdHover @payload={{hash name="utilizationChange" level="is"}}>
          {{percentage this.analysis.isLevelTotals.utilizationChangeTotal decimals=2}}
        </TdHover>
      </tr>
    </tbody>
  {{/if}}
  {{#if this.analysis.hsAnalysis}}
    <tbody>
      <tr>
        <td>
          <strong>
            High Schools
          </strong>
        </td>
        <td>
          {{this.analysis.hsLevelTotals.enrollNoActionTotal}}
        </td>
        <TdHover @payload={{hash name="studentsFromDev" level="hs"}}>
          {{this.analysis.estHsStudents}}
        </TdHover>
        <td>
          {{this.analysis.hsLevelTotals.enrollWithActionTotal}}
        </td>
        <td>
          {{this.analysis.hsLevelTotals.capacityWithActionTotal}}
        </td>
        <td>
          {{this.analysis.hsLevelTotals.seatsWithActionTotal}}
        </td>
        <TdHover @payload={{hash name="utilizationWithAction" level="hs"}}>
          {{percentage this.analysis.hsLevelTotals.utilizationWithActionTotal}}
        </TdHover>
        <TdHover @payload={{hash name="utilizationChange" level="hs"}}>
          {{percentage this.analysis.hsLevelTotals.utilizationChangeTotal decimals=2}}
        </TdHover>
      </tr>
    </tbody>
  {{/if}}
</table>