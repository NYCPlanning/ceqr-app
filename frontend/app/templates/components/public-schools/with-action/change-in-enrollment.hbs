<h4 class="ui header attached top">
  Estimated School Enrollment, Capacity, and Utilization
  {{#if analysis.projectionOverMax}}
    <div data-tooltip="Build year beyond maximum projections available. Using {{analysis.maxProjection}} enrollment projection." data-inverted="" class="ui orange label">
      <i class="warning icon"></i> {{analysis.maxProjection}}
    </div>
  {{/if}} 
</h4>

{{! template-lint-disable table-groups }}
<table class="ui attached bottom celled table">
  <thead>
    <tr>
      <th class="two wide">School Level</th>
      <th class="two wide">Projected Enrollment Without Project</th>
      <th class="two wide">Students Generated by Project</th>
      <th class="one wide">Projected Enrollment with Project</th>
      <th class="one wide">Projected Capacity</th>
      <th class="one wide">Available Seats</th>
      <th class="one wide">Utilization (%) with Project</th>
      <th class="two wide">Change in Utilization (%) from No Action</th>
    </tr>
  </thead>

  {{#if psTable}}
    <tbody>
      <tr>
        <td><strong>Primary Schools</strong></td>
        <td>{{psTable.enrollNoActionTotal}}</td>
        {{#td-hover payload=(hash name="studentsFromDev" level="ps")}}{{analysis.estEsStudents}}{{/td-hover}}
        <td>{{psTable.enrollWithActionTotal}}</td>
        <td>{{psTable.capacityWithActionTotal}}</td>
        <td>{{psTable.seatsWithActionTotal}}</td>
        {{#td-hover payload=(hash name="utilizationWithAction" level="ps")}}{{percentage psTable.utilizationWithActionTotal}}{{/td-hover}}
        {{#td-hover payload=(hash name="utilizationChange" level="ps")}}{{percentage psTable.utilizationChangeTotal decimals=2}}{{/td-hover}}
      </tr>
    </tbody>
  {{/if}}

  {{#if isTable}}
    <tbody>
      <tr>
        <td><strong>Intermediate Schools</strong></td>
        <td>{{isTable.enrollNoActionTotal}}</td>
        {{#td-hover payload=(hash name="studentsFromDev" level="is")}}{{analysis.estIsStudents}}{{/td-hover}}
        <td>{{isTable.enrollWithActionTotal}}</td>
        <td>{{isTable.capacityWithActionTotal}}</td>
        <td>{{isTable.seatsWithActionTotal}}</td>
        {{#td-hover payload=(hash name="utilizationWithAction" level="is")}}{{percentage isTable.utilizationWithActionTotal}}{{/td-hover}}
        {{#td-hover payload=(hash name="utilizationChange" level="is")}}{{percentage isTable.utilizationChangeTotal decimals=2}}{{/td-hover}}
      </tr>
    </tbody>
  {{/if}}

  {{#if analysis.hsAnalysis}}
    <tbody>
      <tr>
        <td><strong>High Schools</strong></td>
        <td>{{hsTable.enrollNoActionTotal}}</td>
        {{#td-hover payload=(hash name="studentsFromDev" level="hs")}}{{analysis.estHsStudents}}{{/td-hover}}
        <td>{{hsTable.enrollWithActionTotal}}</td>
        <td>{{hsTable.capacityWithActionTotal}}</td>
        <td>{{hsTable.seatsWithActionTotal}}</td>
        {{#td-hover payload=(hash name="utilizationWithAction" level="hs")}}{{percentage hsTable.utilizationWithActionTotal}}{{/td-hover}}
        {{#td-hover payload=(hash name="utilizationChange" level="hs")}}{{percentage hsTable.utilizationChangeTotal decimals=2}}{{/td-hover}}
      </tr>
    </tbody>
  {{/if}}
</table>
