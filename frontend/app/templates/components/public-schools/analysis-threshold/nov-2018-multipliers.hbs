<table class="ui attached top table">
  <thead>
    <tr>
      <th colspan="2"></th>
      <th colspan="3">
        Estimated Public School Students Generated by New Residential Units
      </th>
      <th colspan="2">
        Minimum Number of Residential Units
        <br />
        that Trigger Detailed Analyses
      </th>
    </tr>
    <tr>
      <th></th>
      <th>
        School
        <br />
        District
      </th>
      <th>
        Primary School
        <br />
        level per unit
        <br />
        (Age 4-10)
      </th>
      <th>
        Intermediate School
        <br />
        level per unit
        <br />
        (Age 11-13)
      </th>
      <th>
        High School
        <br />
        level per unit
        <br />
        (Age 14-17)
      </th>
      <th>
        Primary/
        <br />
        Intermediate
      </th>
      <th>
        High School
      </th>
    </tr>
  </thead>
  <tbody>
    {{#each multipliers.mn.districts as |m idx|}}
      <tr class="{{m.csd}} {{if (eq m.csd analysis.district) "active"}}">
        {{#if (eq idx 0)}}
          <td rowspan={{multipliers.mn.count}}>
            Manhattan
          </td>
        {{/if}}
        <td>
          {{m.csd}}
        </td>
        <td>
          {{m.ps}}
        </td>
        <td>
          {{m.is}}
        </td>
        <td>
          {{m.hs}}
        </td>
        <td>
          {{m.psisThreshold}}
        </td>
        <td>
          {{m.hsThreshold}}
        </td>
      </tr>
    {{/each}}
