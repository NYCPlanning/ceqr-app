<table class="ui attached top table">
  <thead>
    <tr>
      <th colspan="2"></th>
      <th colspan="3">
        Estimated Public School Students Generated by New Residential Units
      </th>
      <th colspan="2">
        Minimum Number of Residential Units
        <br>
        that Trigger Detailed Analyses
      </th>
    </tr>
    <tr>
      <th></th>
      <th>
        School
        <br>
        District
      </th>
      <th>
        Primary School
        <br>
        level per unit
        <br>
        (Age 4-10)
      </th>
      <th>
        Intermediate School
        <br>
        level per unit
        <br>
        (Age 11-13)
      </th>
      <th>
        High School
        <br>
        level per unit
        <br>
        (Age 14-17)
      </th>
      <th>
        Primary/
        <br>
        Intermediate
      </th>
      <th>
        High School
      </th>
    </tr>
  </thead>

  <tbody>
    {{#each this.multipliers.mn.districts as |m idx|}}
      <tr class="{{m.csd}} {{if (eq m.csd this.analysis.district) "active"}}">
        {{#if (eq idx 0)}}
          <td rowspan={{this.multipliers.mn.count}}>
            Manhattan
          </td>
        {{/if}}
        <td>
          {{m.csd}}
        </td>
        <td>
          {{round m.ps decimals=2}}
        </td>
        <td>
          {{round m.is decimals=2}}
        </td>
        <td>
          {{m.hs}}
        </td>
        <td>
          {{m.psisThreshold}}
        </td>
        <td>
          {{m.hsThreshold}}
        </td>
      </tr>
    {{/each}}
    {{#each this.multipliers.bx.districts as |m idx|}}
      <tr class="{{m.csd}} {{if (eq m.csd this.analysis.district) "active"}}">
        {{#if (eq idx 0)}}
          <td rowspan={{this.multipliers.bx.count}}>
            Bronx
          </td>
        {{/if}}
        <td>
          {{m.csd}}
        </td>
        <td>
          {{round m.ps decimals=2}}
        </td>
        <td>
          {{round m.is decimals=2}}
        </td>
        <td>
          {{m.hs}}
        </td>
        <td>
          {{m.psisThreshold}}
        </td>
        <td>
          {{m.hsThreshold}}
        </td>
      </tr>
    {{/each}}
    {{#each this.multipliers.bk.districts as |m idx|}}
      <tr class="{{m.csd}} {{if (eq m.csd this.analysis.district) "active"}}">
        {{#if (eq idx 0)}}
          <td rowspan={{this.multipliers.bk.count}}>
            Brooklyn
          </td>
        {{/if}}
        <td>
          {{m.csd}}
        </td>
        <td>
          {{round m.ps decimals=2}}
        </td>
        <td>
          {{round m.is decimals=2}}
        </td>
        <td>
          {{m.hs}}
        </td>
        <td>
          {{m.psisThreshold}}
        </td>
        <td>
          {{m.hsThreshold}}
        </td>
      </tr>
    {{/each}}
    {{#each this.multipliers.qn.districts as |m idx|}}
      <tr class="{{m.csd}} {{if (eq m.csd this.analysis.district) "active"}}">
        {{#if (eq idx 0)}}
          <td rowspan={{this.multipliers.qn.count}}>
            Queens
          </td>
        {{/if}}
        <td>
          {{m.csd}}
        </td>
        <td>
          {{round m.ps decimals=2}}
        </td>
        <td>
          {{round m.is decimals=2}}
        </td>
        <td>
          {{m.hs}}
        </td>
        <td>
          {{m.psisThreshold}}
        </td>
        <td>
          {{m.hsThreshold}}
        </td>
      </tr>
    {{/each}}
    {{#each this.multipliers.si.districts as |m idx|}}
      <tr class="{{m.csd}} {{if (eq m.csd this.analysis.district) "active"}}">
        {{#if (eq idx 0)}}
          <td rowspan={{this.multipliers.si.count}}>
            Staten Island
          </td>
        {{/if}}
        <td>
          {{m.csd}}
        </td>
        <td>
          {{round m.ps decimals=2}}
        </td>
        <td>
          {{round m.is decimals=2}}
        </td>
        <td>
          {{m.hs}}
        </td>
        <td>
          {{m.psisThreshold}}
        </td>
        <td>
          {{m.hsThreshold}}
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>

<div class="ui attached bottom icon message">
  <i class="database icon"></i>
  <div class="content">
    <div class="header">
      <a
        target="_blank" rel="noopener noreferrer" href="http://www.nycsca.org/Community/Capital-Plan-Reports-Data#Housing-Projections-70"
      >
        SCA Projected Public School Ratio [{{this.year}}]
        <i class="external alternate icon"></i>
      </a>
    </div>
    <p>
      Multipliers defined by the Schools Construction Authority.
    </p>
  </div>
</div>